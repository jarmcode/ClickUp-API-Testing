{
  "name": "clickupautomation",
  "version": "1.0.0",
  "description": "Click up API automation framework",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js -p default",
    "testWip": "cucumber-js -p default --tags @wip",
    "testApi": "cucumber-js -p default --tags @api --parallel 3",
    "testFolder": "cucumber-js -p default --tags @folder",
    "testList": "cucumber-js -p default --tags @list",
    "testSpace": "cucumber-js -p default --tags @space",
    "report": "node html_reporter.js",
    "check": "eslint **/*.js",
    "fix": "eslint **/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jarmcode/ClickUp-API-Testing.git"
  },
  "author": "AT-17",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^8.7.0",
    "axios": "^1.1.3",
    "chromedriver": "^107.0.3",
    "cucumber-html-reporter": "^5.5.0",
    "jest": "^29.2.2",
    "jsonschema": "^1.4.1",
    "log4js": "^6.7.0",
    "selenium-webdriver": "^4.6.1"
  },
  "devDependencies": {
    "eslint": "^8.26.0"
  }
}
